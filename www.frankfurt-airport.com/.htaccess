# /fra-schedule/www.frankfurt-airport.com/.htaccess

<IfModule mod_rewrite.c>
RewriteEngine on
RewriteCond %{REQUEST_URI} www.frankfurt-airport.com
RewriteRule (arrival|departure)s\.json        index.php
RewriteRule (arrival|departure)s\.cargo\.json index.php?cargo&%{QUERY_STRING}
RewriteCond %{QUERY_STRING} ^$
RewriteRule (de|en|cn)/.*(air(craft|line|port)s)\.json json/1.2.5/flights_copy.$2.json [NC,L]
RewriteCond %{QUERY_STRING} local
RewriteRule (de|en|cn)/.*(air(craft|line|port)s)\.json$ json/airports.json [NC,L]
</IfModule>
